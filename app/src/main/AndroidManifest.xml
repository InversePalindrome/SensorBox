<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inversepalindrome.sensorbox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Launcher">
        <activity android:name="com.inversepalindrome.sensorbox.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.inversepalindrome.sensorbox.CompassActivity" android:label="Compass" />
        <activity android:name="com.inversepalindrome.sensorbox.AccelerometerActivity" android:label="Accelerometer"></activity>
    </application>

</manifest>